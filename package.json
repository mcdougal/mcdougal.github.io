{
  "name": "personal-site",
  "version": "1.0.0",
  "description": "Personal website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcdougal/personal-site.git"
  },
  "scripts": {
    "build": "next build src",
    "dev": "next src",
    "export": "next export src -o './out' && touch out/.nojekyll",
    "lint": "run-s lint-**",
    "lint-css": "stylelint --config .stylelintrc './src/**/*.css'",
    "lint-js": "NODE_ENV=development eslint './src/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "js-isort --config-path .babelrc.js",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.css": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "css-to-mui-loader": "^1.3.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsdoc": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^2.7.0",
    "js-isort": "^2.0.4",
    "lint-staged": "^8.2.1",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
